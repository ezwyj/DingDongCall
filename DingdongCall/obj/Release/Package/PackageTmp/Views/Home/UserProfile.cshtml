
@{
    ViewBag.Title = "配置信息";
}


    


<div class="weui-panel">
    <div class="weui-panel__hd">设置手机号(<i class="weui-icon-info-circle">i</i>目前只支持查找一台手机)</div>
    <div class="weui-panel__bd">
        <div class="weui-cells weui-cells_form">
            <div class="weui-panel__bd">
                <div class="weui-media-box weui-media-box_text">
                    <h4 class="weui-media-box__title"><label class="weui-label">手机号</label></h4>
                    <p class="weui-media-box__desc"><input class="weui-input" type="text" placeholder="请输入13位手机号" id="phone" value="@ViewBag.phone" /></p>
                </div>

            </div>

            <div class="weui-btn-area">
                <a class="weui-btn weui-btn_primary" id="ok">确定</a>
            </div>
            <input type="hidden" id="userid" value="@ViewBag.userId" />
            
                
            
        </div>
    </div>
</div>
    @section scripts {
        <script type="text/javascript">
            OP_CONFIG.module = 'home';
            OP_CONFIG.page = 'userProfile';

        </script>
    }
